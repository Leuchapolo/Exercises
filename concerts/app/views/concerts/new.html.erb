<% if @new_concert.errors.any? %>
	ERROR
	<br>
	<% @new_concert.errors.full_messages.each do |err| %>
		<%= err %><br>
	<% end %>
<% end %>
<br> <br>

<%= form_for @new_concert do |f|%> 
	<%= f.label :artist%>
	<%= f.text_field :artist%>
	<%= f.label :venue%>
	<%= f.text_field :venue%>
	<%= f.label :city%>
	<%= f.text_field :city%>
	<%= f.label :date%>
	<%= f.datetime_select :date%>
	<%= f.label :price%>
	<%= f.text_field :price%>
	<%= f.label :description%>
	<%= f.text_area :description%>
	<%= f.submit %>
<% end %> 
